@page
@model CommunityApp.Pages.Cities.CreateModel

<h2>Add New City</h2>

<form method="post">
    <label>City Name</label>
    <input type="text" asp-for="City.CityName" required />

    <label>Population</label>
    <input type="number" asp-for="City.Population" required />

    <label>Province Code</label>
    <input type="text" asp-for="City.ProvinceCode" required />

    <button type="submit">Save</button>
</form>

<a asp-page="./Index">Back to List</a>
