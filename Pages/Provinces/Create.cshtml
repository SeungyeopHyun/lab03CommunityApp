@page
@model CommunityApp.Pages.Provinces.CreateModel

<h2>Add New Province</h2>

<form method="post">
    <div>
        <label>Province Code</label>
        <input type="text" asp-for="Province.ProvinceCode" required />
        <span asp-validation-for="Province.ProvinceCode" class="text-danger"></span>
    </div>

    <div>
        <label>Province Name</label>
        <input type="text" asp-for="Province.ProvinceName" required />
    </div>

    <button type="submit" class="btn btn-success">Save</button>
</form>

<a asp-page="./Index">Back to List</a>
